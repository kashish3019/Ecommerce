    <style>
        body {
            font-family: Arial, sans-serif;

        }

        #box img {
            width: 200px;
            height: 200px;
        }

        #box {
            display: grid;
            grid-template-columns: repeat(3, 1fr);
            margin-top: 50px;
        }

        .banner img {
            height: 100%;
            width: 100%;
        }

        .container {
            max-width: 1140px;
            margin: auto;
        }

        .w-25 {
            width: 25%;
            padding: 15px;
        }

        .w-33 {
            width: 33%;
            padding: 15px;
        }

        .w-50 {
            width: 50%;
        }

        .w-60 {
            width: 60%;
        }

        .w-40 {
            width: 40%;
        }

        .row {
            display: flex;
            flex-wrap: nowrap;
        }

        /* banner-2 section  */
        .banner-2 {
            padding: 50px;
        }

        .banner-2 .banners {
            position: relative;
            overflow: hidden;
            margin: 10px;
        }

        .banner-2 .banners img {
            width: 100%;
            height: 100%;
            object-fit: cover;
            transition: transform 0.3s ease-in-out;
        }

        .banner-2 .banners .overlay {
            position: absolute;
            bottom: 100%;
            left: 0;
            right: 0;
            background-color: rgba(85, 89, 91, 0.379);
            overflow: hidden;
            width: 100%;
            height: 0;
            opacity: 0;
            transition: .5s ease;
        }

        .banner-2 .banners:hover .overlay {
            bottom: 0;
            height: 100%;
            opacity: 1;
        }

        /* product section  */
        .heading {
            text-align: center;
            padding: 10px;
        }

        .chair-icon {
            position: relative;
            text-align: center;
            font-size: 30px;
            padding: 10px;
        }

        .chair-icon i::after {
            position: absolute;
            content: "";
            width: 250px;
            height: 3px;
            padding: 1px;
            border-top: 1px solid rgba(128, 128, 128, 0.304);
            border-bottom: 1px solid rgba(128, 128, 128, 0.304);
            bottom: 0;
            top: 5px;
            margin: auto;
            left: 10px;
            z-index: -1;
            right: 0;
        }

        .chair-icon i::before {
            background-color: white;
            padding-right: 20px;
            padding-left: 20px;
            position: absolute;
            right: 550px;
            left: 545px;
            width: 20px;
            top: 15px;
            z-index: 999;
        }

        /* heading section  */
        .heading-2 {
            justify-content: center;
            text-align: center;
            width: 390px;
            margin: auto;
            padding-top: 5px;
        }

        .heading-2 ul {
            list-style: none;
        }

        .heading-2 ul li {
            padding: 20px;
            margin: 10px;
            transition: all 0.5s;
        }

        .heading-2 ul li:hover {
            background-color: #ffce24;
        }

        /* furniture section  */
        .furniture {
            text-align: center;
            padding-top: 50px;
        }

        .furniture h4 {
            font-weight: 300;
            letter-spacing: 1px;
            word-spacing: 5px;
            padding-bottom: 25px;
        }

        .furniture h2 {
            font-size: 55px;
            font-weight: 400;
            line-height: 55px;
            padding: 20px;
        }

        .furniture h2 span {
            font-size: 45px;
        }

        .furniture p {
            font-size: 15px;
            padding: 10px 35px;
            line-height: 25px;
            color: #666666;
            letter-spacing: 1px;
            word-spacing: 2px;
        }

        .furniture button {
            background-color: #ffce24;
            border: none;
            padding: 15px 30px;
            font-size: 15px;
            margin: 20px;
            transition: all 0.5s;
        }
        .furniture button:hover {
            background-color: black;
            color: white;
        }

        .img-furniture {
            background-color: #4e4e4e;
            margin-left: 120px;
            position: relative;
            width: 450px;
            height: 600px;
        }

        .img-furniture .img-box1 {
            position: absolute;
            top: 80px;
            right: 250px;
        }

        .img-furniture .img-box2 {
            position: absolute;
            top: 80px;
            right: -100px;
        }

        .img-furniture .img-box1 img,
        .img-furniture .img-box2 img {
            width: 300px;
        }


        .icon-hover {
            position: absolute;
            bottom: 0;
            height: 450px;
            width: 300px;
            opacity: 0;
            transition: .2s ease;
            background-color: rgba(23, 22, 22, 0.169);
        }

        .img-box1:hover .icon-hover,
        .img-box2:hover .icon-hover {
            opacity: 1;
            margin-bottom: 5px;
        }

        .img-furniture .icon {
            color: white;
            font-size: 30px;
            position: absolute;
            top: 50%;
            left: 50%;
            -webkit-transform: translate(-50%, -50%);
            -ms-transform: translate(-50%, -50%);
            transform: translate(-50%, -50%);

        }

        /* product images section css */
        .product-box {
            margin: 50px 0;
        }

        .main-image-div {
            position: relative;
        }

        .main-image-div .product-img img {
            width: 100%;
            height: 100%;
            object-fit: cover;
            object-position: center;
        }

        .main-image-div .image-div {
            position: absolute;
            width: 50px;
            height: 80px;
            z-index: 9;
            border: 3px solid white;
            right: 5px;
            bottom: 8px;
            transition: all 0.5s;
        }

        .main-image-div .image-div img {
            width: 100%;
            height: 100%;
            object-fit: cover;
        }

        .main-image-div:hover .image-div {
            width: 100%;
            height: 100%;
            right: 0px;
            bottom: 0px;
        }

        .main-image-div .icon-div {
            position: absolute;
            top: 10px;
            right: 50px;
            z-index: 99;
            visibility: hidden;
            opacity: 0;
            transition: all 0.2s;
        }

        .main-image-div:hover .icon-div {
            visibility: visible;
            opacity: 1;
        }

        .main-image-div .icon-div i {
            position: absolute;
            background-color: #366e78;
            color: white;
            width: 40px;
            height: 40px;
            font-size: 20px;
            text-align: center;
            line-height: 40px;
        }

        .main-image-div .icon-div i:first-child {
            transition: all 0.2s;
        }

        .main-image-div .icon-div i:nth-child(2) {
            transition: all 0.5s;
        }

        .main-image-div .icon-div i:nth-child(3) {
            transition: all 0.8s;
        }

        .main-image-div .icon-div i:last-child {
            transition: all 1s;
        }

        .main-image-div:hover .icon-div i:nth-child(2) {
            top: 40px;
        }

        .main-image-div:hover .icon-div i:nth-child(3) {
            top: 80px;
        }

        .main-image-div:hover .icon-div i:last-child {
            top: 120px;
        }

        .product-box .image-content {
            text-align: center;
            padding: 10px;
        }

