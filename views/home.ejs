        .news .latest-news .latest-news-overlay {
            position: absolute;
            width: 100%;
            height: 100%;
            background-color: rgba(0, 0, 0, 0.335);
            opacity: 0;
            visibility: hidden;
            transition: all 0.5s;
        }

        .news .latest-news .latest-news-overlay i {
            width: 60px;
            height: 60px;
            border: 2px solid white;
            color: white;
            font-size: 20px;
            text-align: center;
            line-height: 60px;
            border-radius: 50%;
            position: absolute;
            left: 40%;
            right: 40%;
            top: 40%;
            bottom: 40%;
            transition: all 0.3s;
        }

        .news .latest-news:hover .latest-news-overlay {
            opacity: 1;
            visibility: visible;
        }

        .news .latest-news:hover .latest-news-overlay i {
            width: 40px;
            height: 40px;
            line-height: 40px;
            font-size: 16px;
            left: 43%;
            right: 47%;
            top: 43%;
            bottom: 47%;
